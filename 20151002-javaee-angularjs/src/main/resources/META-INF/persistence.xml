<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0">

  <persistence-unit name="test" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>fr.speedyfleet.model.Utilisateur</class>
    <class>fr.speedyfleet.model.backoffice.FonctionBO</class>
    <class>fr.speedyfleet.model.backoffice.CompteGroupeBO</class>
    <class>fr.speedyfleet.model.backoffice.ContactBO</class>
    <class>fr.speedyfleet.model.backoffice.AgenceBO</class>
    <class>fr.speedyfleet.model.backoffice.LienVehiculeBO</class>
    <class>fr.speedyfleet.model.backoffice.DonneeComplemBO</class>
    <class>fr.speedyfleet.model.backoffice.DonneeComplemLibelleBO</class>
    <class>fr.speedyfleet.model.backoffice.GardiennageBO</class>
    <class>fr.speedyfleet.model.backoffice.CentreDeStockageBO</class>
    <class>fr.speedyfleet.model.backoffice.DotationPneuBO</class>
    <class>fr.speedyfleet.model.backoffice.DotationPneuDemandeDepassBO</class>
    <class>fr.speedyfleet.model.DotationPneuContacts</class>
    <class>fr.speedyfleet.model.backoffice.FactureEnteteComptantBO</class>
    <class>fr.speedyfleet.model.backoffice.FactureDetailComptantBO</class>
    <class>fr.speedyfleet.model.backoffice.FactureEnteteFlotteBO</class>
    <class>fr.speedyfleet.model.backoffice.FactureDetailFlotteBO</class>
    <properties>
      <property name="hibernate.dialect" value="org.hibernate.dialect.HSQLDialect" />
    </properties>
    
  </persistence-unit>
    <persistence-unit name="prod" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>fr.speedyfleet.model.Utilisateur</class>
    <class>fr.speedyfleet.model.backoffice.FonctionBO</class>
    <class>fr.speedyfleet.model.backoffice.CompteGroupeBO</class>
    <class>fr.speedyfleet.model.backoffice.ContactBO</class>
    <class>fr.speedyfleet.model.backoffice.AgenceBO</class>
    <class>fr.speedyfleet.model.backoffice.LienVehiculeBO</class>
    <class>fr.speedyfleet.model.backoffice.DonneeComplemBO</class>
    <class>fr.speedyfleet.model.backoffice.DonneeComplemLibelleBO</class>
    <class>fr.speedyfleet.model.backoffice.GardiennageBO</class>
    <class>fr.speedyfleet.model.backoffice.CentreDeStockageBO</class>
    <class>fr.speedyfleet.model.backoffice.DotationPneuBO</class>
    <class>fr.speedyfleet.model.backoffice.DotationPneuDemandeDepassBO</class>
    <class>fr.speedyfleet.model.DotationPneuContacts</class>
    <class>fr.speedyfleet.model.backoffice.FactureEnteteComptantBO</class>
    <class>fr.speedyfleet.model.backoffice.FactureDetailComptantBO</class>
    <class>fr.speedyfleet.model.backoffice.FactureEnteteFlotteBO</class>
